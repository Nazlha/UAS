in_window = tkinter.Tk()

def event_klik():
    label2 = tkinter.Label(main_window, text = "hai kamu")
    label2.pack()

    
label = tkinter.Label(main_window, text = "Nazlha Noor Aiena Sofhie \n 20210801281")
tombol = tkinter.Button(main_window, text = "klik ini", command = event_klik)

label.pack()
tombol.pack()
main_window.mainloop()
